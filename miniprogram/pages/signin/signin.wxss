.signin{
  padding: 30rpx;
  background: #f5f5f5;
  height: 100%;
  box-sizing: border-box;
}
.item{
  background: #fff;  
  padding: 0 30rpx 50rpx 30rpx;
  border-radius: 10rpx;
  box-shadow: 0 7rpx 14rpx #dcdcdc;
  margin-bottom: 50rpx;
}
.item-head{
  display: flex;
  justify-content: space-between; 
  line-height: 46rpx;  
}
.item-head-left{
  color: #fff;
  text-align: center;
  padding: 0 10rpx;
  background: #00a3e7;
}
.item-head-right{}

.item-info{
  display: flex;
  justify-content: space-between;
  line-height: 70rpx;
  padding: 40rpx 0 15rpx 0;
}
.item-info-left{
  font-size: 42rpx;  
  color: #00a3e7;
}
.item-info-right{
  color: #999;
  font-size: 32rpx;  
}
.item-info-date{}
.item-info-count{
  display: inline-block;
  margin-left: 20rpx;
}
.item-info-describe{
  color: #666;
  padding-left: 15rpx;
}
.add-btn{
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
}
.icon-remove-parent{
  width: 70rpx;
  height: 40rpx;
  text-align: right;
}
.icon-remove{
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-repeat: no-repeat;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
}
.icon-add{
  background-image: url("data:image/png;base64,");
    background-size: 100%;
  background-repeat: no-repeat;
  width: 100rpx;
  height: 100rpx;
  z-index: 1000;
}