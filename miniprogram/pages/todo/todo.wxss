/* 覆盖有赞按钮样式 */
.vant-btn{
  padding: 0 0!important;
  border-radius: 8rpx!important;
  min-width:80rpx!important;
}
.col-left .van-cell{
  width: 600rpx!important;
}
.btn-cut{
  margin: 0 15rpx!important;
}
.done-checkbox .van-checkbox__label{
  color:#999;
  text-decoration: line-through;
}
.icon-width{
  width: 80rpx;
}
.van-button--primary{
  background: #00a3e7!important;
  border-color: #00a3e7!important;
}

.all-list{
}
.title{
  line-height: 100rpx;
  color: #999;
  border-bottom: 1rpx solid #dcdcdc;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
}
.show-icon{
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
}
.list{
  padding: 0 30rpx;
}
.row{
  display: flex;
  padding: 15rpx 0;
  justify-content: space-between;
  box-sizing: border-box;
  border-bottom: 1rpx solid #dcdcdc;
}
.row:last-child{
  border-bottom: 1rpx solid #dcdcdc;  
}
.col-left{
  flex: 1;
  display: flex;
  align-items: center;
}
.col-right{
  display: flex;
  align-items: center;
}


